<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.rs.teach.mapper.node.dao.CourseNodeMapper">

    <insert id="saveNode" parameterType="com.rs.teach.mapper.node.entity.CourseNode">
        insert into tf_el_note(course_Id,user_Id,section_Id,note,class_Id) values (#{courseId},#{userId},#{sectionId},#{note},#{classId})
    </insert>

    <select id="selectNode"  resultType="com.rs.teach.mapper.node.entity.CourseNode">
        select * from tf_el_note
        where user_Id = #{userId}
        and section_Id = #{sectionId}
        and course_Id = #{trainCourseId}
    </select>
</mapper>